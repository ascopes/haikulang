use std;

struct Factorial {
    count: uint;
    value: std::BigInt;
}

fn factorial(n: uint) -> Factorial {
    let value = BigInt.new(n);
    let count = 1;

    while (count < n) {
        count += 1;
        value = value.mul(count);
    }

    return Factorial(count, value);  // todo: implement struct init syntax?
}

fn main() {
    println("{}", factorial(50));
}
